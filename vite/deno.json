    "@types/react": "npm:@types/react@^18",
    "@types/react-dom": "npm:@types/react-dom@^18",
    "@vitejs/plugin-react": "npm:@vitejs/plugin-react@^4.3.2",
    "autoprefixer": "npm:autoprefixer@^10",
    "class-variance-authority": "npm:class-variance-authority@^0.7.0",
    "clsx": "npm:clsx@^2.1.1",
    "framer-motion": "npm:framer-motion@^11.11.9",
    "jotai": "npm:jotai@^2.10.1",
    "postcss": "npm:postcss@^8",
    "react": "npm:react@^18",
    "react-dom": "npm:react-dom@^18",
    "react-markdown": "npm:react-markdown@^9.0.1",
