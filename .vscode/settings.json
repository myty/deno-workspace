{
    "deno.enablePaths": [
      "./"
    ],
    "deno.enable": true,
    "deno.lint": true,
    "editor.inlayHints.enabled": "off",
    "editor.defaultFormatter": "denoland.vscode-deno",
    "[typescriptreact]": {
      "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "[typescript]": {
      "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "[javascriptreact]": {
      "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "[javascript]": {
      "editor.defaultFormatter": "denoland.vscode-deno"
    },
    "files.associations": {
      "*.tsx": "typescriptreact",
      "*.css": "tailwindcss",
      "*.scss": "tailwindcss"
    },
    "[tailwindcss]": {
      "tailwindCSS.colorDecorators": true
    },
    "tailwindCSS.experimental.classRegex": [
      ["clsx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"],
      ["cva\\(([^)]*)\\)", "[\"'`]([^\"'`]*).*?[\"'`]"],
      ["cx\\(([^)]*)\\)", "(?:'|\"|`)([^']*)(?:'|\"|`)"]
    ],
    "trailing-spaces.trimOnSave": true,
    "editor.stickyScroll.enabled": true,
    "codevoid.inner-loop-buddy.defaultUrl": "http://localhost:5173",
}
